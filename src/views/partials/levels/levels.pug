ul 
  each item in levels 
    li(class='mx-auto my-8 p-8 bg-altBgColor/30 flex justify-between') 
      div(class="px-4")
        h3.h3=item.data.type
        p.p 
          strong Price: 
          |#{item.data.price} $

        a.button(href="#buy" class="block w-fit mt-8") Book
      p.p(class="px-4")
        strong Diescription: 
        =item.data.description
      div(class="px-4")
        p.p 
          strong Included: 
        ul(class="pl-8 list-disc")
          each included in item.data.included
            li
              p.p #{included}